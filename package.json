{
  "name": "clean_typeorm_api_server",
  "version": "0.1.0",
  "description": "TypeOrm v0.3.16 server api",
  "main": "Index.ts",
  "scripts": {
    "start": "npm run build && cd dist && cross-env NODE_ENV=PRODUCTION node -r dotenv/config Index.js",
    "dev": "nodemon",
    "test": "cross-env NODE_ENV=test mocha test/**/*.ts --timeout 5000",
    "build": "npm run clean && tsc --outDir dist && cp .env ./dist",
    "clean": "rimraf dist",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ormconfig.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./ormconfig.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./ormconfig.ts migration:generate ./src/migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./ormconfig.ts migration:revert"
  },
  "keywords": [
    "node",
    "express",
    "typeorm",
    "typescript",
    "tslint",
    "JWT",
    "E2015"
  ],
  "author": "Yaniv Duke",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "cluster": "^0.7.7",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.0",
    "messagebird": "^4.0.1",
    "method-override": "^3.0.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mustache": "^4.2.0",
    "nanoid": "^3.1.20",
    "nodemailer": "^6.9.3",
    "nodemailer-smtp-transport": "^2.7.4",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "pg": "^8.11.0",
    "superagent": "^8.0.9",
    "token-extractor": "^0.1.6",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.16",
    "typescript": "^5.1.3",
    "uuid": "^9.0.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/method-override": "^0.0.32",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/mustache": "^4.2.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.8",
    "@types/nodemailer-smtp-transport": "^2.7.5",
    "@types/superagent": "^4.1.18",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@types/web-push": "^3.3.2",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3"
  }
}
